<h1>login</h1>

<form action="/login" method="post">
    <div>
        <label for="email">EMAIL</label>
        <input type="email" name="email" required>
    </div>
    <div>
        <label for="password">PASSWORD</label>
        <input type="password" name="password" required>
    </div>
    <div>
        <button type="submit">submit</button>
    </div>

</form>